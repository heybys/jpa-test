### Successful POST users: check response status is 201
POST {{url}}/api/v1/users
Content-Type: application/json

{
  "name": "test-name-{{$uuid}}",
  "type": "NORMAL",
  "phoneNumber": "010{{$randomInt}}{{$randomInt}}{{$randomInt}}",
  "address": "Gangseo-gu, Seoul, Republic of Korea"
}

> {%
client.test("User created successfully", function() {
  client.assert(response.status === 201, "Response status is not 201");
  client.assert(response.body["statusCode"] === "SUCCESS", "Response body.statusCode is not SUCCESS");
  client.assert(response.body["data"]["id"], "Response body.data.id should not be undefined");
});
%}

### Failed POST users: check response status is 400
POST {{url}}/api/v1/users
Content-Type: application/json

{
  "username": null
}

> {%
client.test("User created unsuccessfully", function() {
  client.assert(response.status === 400, "Response status is not 400");
  client.assert(response.body["statusCode"] === "FAIL", "Response body.statusCode is not FAIL");
});
%}

